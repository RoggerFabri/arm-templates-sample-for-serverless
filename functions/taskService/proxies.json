{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "proxyListTasks": {
      "matchCondition": {
        "methods": [
          "GET"
        ],
        "route": "/api/{version}/{userId}/tasks"
      },
      "backendUri": "https://localhost/api/list"
    },
    "proxyCreateTask": {
      "matchCondition": {
        "methods": [
          "POST"
        ],
        "route": "/api/{version}/{userId}/tasks"
      },
      "backendUri": "https://localhost/api/create"
    }
  }
}
